{
  "require": {
		"vlucas/phpdotenv": "^3.3",
    "doctrine/orm": "^2.6.2",
    "almasaeed2010/adminlte": "~2.4"
  },
  "autoload": {
    "psr-0": {"": "src/"}
  },
	"scripts": {
		"post-install-cmd": [
			"php copyAdminlte.php"
		],
		"post-update-cmd": [
			"php copyAdminlte.php"
		],
		"run": [
			"php -S 127.0.0.1:8000 -t src"
		]
	},
	"config": {
    "process-timeout": 0
  }
}
